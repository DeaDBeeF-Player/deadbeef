#: ../src/main.c:140
#, c-format
msgid "Usage: deadbeef [options] [--] [file(s)]\n"
msgstr "Употреба: deadbeef [опције] [--] [датотека(е)] \n"

#: ../src/main.c:141
#, c-format
msgid "Options:\n"
msgstr "Опције:\n"

#: ../src/main.c:142
#, c-format
msgid "   --help  or  -h     Print help (this message) and exit\n"
msgstr "   --help  или  -h     Прикажи ову помоћ и изађи\n"

#: ../src/main.c:143
#, c-format
msgid "   --quit             Quit player\n"
msgstr "   --quit             Напусти плејер\n"

#: ../src/main.c:144
#, c-format
msgid "   --version          Print version info and exit\n"
msgstr "   --version          Прикажи инфо о верзији и изађи\n"

#: ../src/main.c:145
#, c-format
msgid "   --play             Start playback\n"
msgstr "   --play             Репродукуј\n"

#: ../src/main.c:146
#, c-format
msgid "   --stop             Stop playback\n"
msgstr "   --stop             Заустави репродукцију\n"

#: ../src/main.c:147
#, c-format
msgid "   --pause            Pause playback\n"
msgstr "   --pause            Паузирај репродукцију\n"

#: ../src/main.c:148
#, c-format
msgid "   --toggle-pause     Toggle pause\n"
msgstr "   --toggle-pause     Паузирај\n"

#: ../src/main.c:149
#, c-format
msgid ""
"   --play-pause       Start playback if stopped, toggle pause otherwise\n"
msgstr "   --play-pause       Започни репродукцију ако је заустављена, и обратно\n"

#: ../src/main.c:150
#, c-format
msgid "   --next             Next song in playlist\n"
msgstr "   --next             Наредна песма из листе\n"

#: ../src/main.c:151
#, c-format
msgid "   --prev             Previous song in playlist\n"
msgstr "   --prev             Претходна песма из листе\n"

#: ../src/main.c:152
#, c-format
msgid "   --random           Random song in playlist\n"
msgstr "   --random           Насумично песма из листе\n"

#: ../src/main.c:153
#, c-format
msgid "   --queue            Append file(s) to existing playlist\n"
msgstr "   --queue            Додај датотеку(е) у постојећу листу\n"

#: ../src/main.c:154
#, c-format
msgid "   --gui PLUGIN       Tells which GUI plugin to use, default is \"GTK2\"\n"
msgstr "   --gui ДОДАТАК       Додатак граф. сучеља — подразумеван је „ГТК2“\n"

#: ../src/main.c:155
#, c-format
msgid "   --nowplaying FMT   Print formatted track name to stdout\n"
msgstr "   --nowplaying FMT   Испиши уређени назив песме на стд. излаз\n"

#: ../src/main.c:158
#, c-format
msgid ""
"                      FMT %%-syntax: [a]rtist, [t]itle, al[b]um,\n"
"                      [l]ength, track[n]umber, [y]ear, [c]omment,\n"
"                      copy[r]ight, [e]lapsed\n"
msgstr "                      FMT %%-syntax: [a]-извођач, [t]-наслов, [b]-албум,\n                      [l]-трајање, [n]-бр.песме, [y]-година, [c]-примедба,\n                      [r]-ауторска права, [e]преостало време\n"

#: ../src/main.c:161
#, c-format
msgid ""
"                      example: --nowplaying \"%%a - %%t\" should print "
"\"artist - title\"\n"
msgstr "                      пример: --nowplaying \"%%a - %%t\" ће исписати „Извођач - Наслов“\n"

#: ../src/main.c:164
#, c-format
msgid "                      for more info, see %s\n"
msgstr "                      за више информација, видите %s\n"

#: ../src/main.c:166
#, c-format
msgid "                      NOTE: --nowplaying is deprecated.\n"
msgstr "                      ПАЖЊА: --nowplaying је застарело\n.\n"

#: ../src/main.c:169
#, c-format
msgid ""
"   --nowplaying-tf FMT  Print formatted track name to stdout, using the new "
"title formatting\n"
msgstr "   --nowplaying-tf FMT  Штампај форматирано име песме на stdout, користећи нови начин форматирања\n"

#: ../src/main.c:172
#, c-format
msgid ""
"                      FMT syntax: http://github.com/DeaDBeeF-"
"Player/deadbeef/wiki/Title-formatting-2.0\n"
msgstr "                      FMT синтакса: http://github.com/DeaDBeeF-Player/deadbeef/wiki/Title-formatting-2.0\n"

#: ../src/main.c:175
#, c-format
msgid ""
"                      example: --nowplaying-tf \"%%artist%% - %%title%%\" "
"should print \"artist - title\"\n"
msgstr "                      пример: --nowplaying-tf \"%%artist%% - %%title%%\" треба да штампа \"извођач - име песма\"\n"

#: ../src/main.c:176
#, c-format
msgid "   --volume [NUM]     Print or set deadbeef volume level.\n"
msgstr "   --volume [NUM]     Штампај или постави deadbeef ниво јачине звука.\n"

#: ../src/main.c:179
#, c-format
msgid ""
"                      The NUM parameter can be specified in percents "
"(absolute value or increment/decrement)\n"
msgstr ""

#: ../src/main.c:180
#, c-format
msgid "                      or in dB [-50, 0] (if with suffix).\n"
msgstr ""

#: ../src/main.c:181
#, c-format
msgid ""
"                      Examples: --volume 80, --volume +10, --volume -5 or "
"--volume -20dB\n"
msgstr ""

#: ../src/main.c:184
#, c-format
msgid ""
"   --plugin=[PLUG]    Send commands to a specific plugin. Use PLUG=main to "
"send commands to deadbeef itself.\n"
msgstr ""

#: ../src/main.c:185
#, c-format
msgid ""
"                      To get plugin specific commands use --plugin=[PLUG] "
"--help\n"
msgstr ""

#: ../src/main.c:188
#, c-format
msgid ""
"   --plugin-list      List all available plugins including indication for "
"plugins that support commands.\n"
msgstr ""

