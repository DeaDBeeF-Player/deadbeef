#: ../src/main.c:140
#, c-format
msgid "Usage: deadbeef [options] [--] [file(s)]\n"
msgstr "Erabilera: deadbeef [aukerak] [--] [fitxategia(k)]\n"

#: ../src/main.c:141
#, c-format
msgid "Options:\n"
msgstr "Aukerak:\n"

#: ../src/main.c:142
#, c-format
msgid "   --help  or  -h     Print help (this message) and exit\n"
msgstr "   --help  or  -h     Inprimatu laguntza (mezu hau) eta irten\n"

#: ../src/main.c:143
#, c-format
msgid "   --quit             Quit player\n"
msgstr "   --quit             Irten erreproduzitzailetik\n"

#: ../src/main.c:144
#, c-format
msgid "   --version          Print version info and exit\n"
msgstr "   --version          Inprimatu bertsio-informazioa eta irten\n"

#: ../src/main.c:145
#, c-format
msgid "   --play             Start playback\n"
msgstr "   --play             Hasi erreprodukzioa\n"

#: ../src/main.c:146
#, c-format
msgid "   --stop             Stop playback\n"
msgstr "   --stop             Gelditu erreprodukzioa\n"

#: ../src/main.c:147
#, c-format
msgid "   --pause            Pause playback\n"
msgstr "   --pause            Pausatu erreprodukzioa\n"

#: ../src/main.c:148
#, c-format
msgid "   --toggle-pause     Toggle pause\n"
msgstr "   --toggle-pause     Txandakatu pausatzea\n"

#: ../src/main.c:149
#, c-format
msgid ""
"   --play-pause       Start playback if stopped, toggle pause otherwise\n"
msgstr "   --play-pause       Hasi erreproduzitzen geldituta badago, bestela txandakatu pausa\n"

#: ../src/main.c:150
#, c-format
msgid "   --next             Next song in playlist\n"
msgstr "   --next             Erreprodukzio-zerrendako hurrengo kanta\n"

#: ../src/main.c:151
#, c-format
msgid "   --prev             Previous song in playlist\n"
msgstr "   --prev             Erreprodukzio-zerrendako aurreko kanta\n"

#: ../src/main.c:152
#, c-format
msgid "   --random           Random song in playlist\n"
msgstr "   --random           Erreprodukzio-zerrendako ausazko kanta\n"

#: ../src/main.c:153
#, c-format
msgid "   --queue            Append file(s) to existing playlist\n"
msgstr "   --queue            Gehitu fitxategia(k) existitzen den erreprodukzio-zerrendara\n"

#: ../src/main.c:154
#, c-format
msgid "   --gui PLUGIN       Tells which GUI plugin to use, default is \"GTK2\"\n"
msgstr "--gui PLUGIN-ek zein GUI plugin erabili esaten du, lehenetsia \"GTK2\" da\n"

#: ../src/main.c:155
#, c-format
msgid "   --nowplaying FMT   Print formatted track name to stdout\n"
msgstr "   --nowplaying FMT   Inprimatu formatudun pista-izena irteera estandarrera\n"

#: ../src/main.c:158
#, c-format
msgid ""
"                      FMT %%-syntax: [a]rtist, [t]itle, al[b]um,\n"
"                      [l]ength, track[n]umber, [y]ear, [c]omment,\n"
"                      copy[r]ight, [e]lapsed\n"
msgstr "                      FMT %%-sintaxia: a: artista, t: izenburua, b: albuma,\n                      l: iraupena, n: pista zenbakia, y: urtea, c: iruzkina,\n                      r: copyright-a, e: igarotakoa\n"

#: ../src/main.c:161
#, c-format
msgid ""
"                      example: --nowplaying \"%%a - %%t\" should print "
"\"artist - title\"\n"
msgstr ""

#: ../src/main.c:164
#, c-format
msgid "                      for more info, see %s\n"
msgstr ""

#: ../src/main.c:166
#, c-format
msgid "                      NOTE: --nowplaying is deprecated.\n"
msgstr ""

#: ../src/main.c:169
#, c-format
msgid ""
"   --nowplaying-tf FMT  Print formatted track name to stdout, using the new "
"title formatting\n"
msgstr ""

#: ../src/main.c:172
#, c-format
msgid ""
"                      FMT syntax: http://github.com/DeaDBeeF-"
"Player/deadbeef/wiki/Title-formatting-2.0\n"
msgstr ""

#: ../src/main.c:175
#, c-format
msgid ""
"                      example: --nowplaying-tf \"%%artist%% - %%title%%\" "
"should print \"artist - title\"\n"
msgstr ""

#: ../src/main.c:176
#, c-format
msgid "   --volume [NUM]     Print or set deadbeef volume level.\n"
msgstr ""

#: ../src/main.c:179
#, c-format
msgid ""
"                      The NUM parameter can be specified in percents "
"(absolute value or increment/decrement)\n"
msgstr ""

#: ../src/main.c:180
#, c-format
msgid "                      or in dB [-50, 0] (if with suffix).\n"
msgstr ""

#: ../src/main.c:181
#, c-format
msgid ""
"                      Examples: --volume 80, --volume +10, --volume -5 or "
"--volume -20dB\n"
msgstr ""

#: ../src/main.c:184
#, c-format
msgid ""
"   --plugin=[PLUG]    Send commands to a specific plugin. Use PLUG=main to "
"send commands to deadbeef itself.\n"
msgstr ""

#: ../src/main.c:185
#, c-format
msgid ""
"                      To get plugin specific commands use --plugin=[PLUG] "
"--help\n"
msgstr ""

#: ../src/main.c:188
#, c-format
msgid ""
"   --plugin-list      List all available plugins including indication for "
"plugins that support commands.\n"
msgstr ""

