#: ../main.c:103
#, c-format
msgid "Usage: deadbeef [options] [--] [file(s)]\n"
msgstr "Використання: deadbeef [опції] [--] [файл(и)]\n"

#: ../main.c:104
#, c-format
msgid "Options:\n"
msgstr "Опції:\n"

#: ../main.c:105
#, c-format
msgid "   --help  or  -h     Print help (this message) and exit\n"
msgstr ""
"   --help  або  -h     Показати цю допомогу (це повідомлення) і вийти\n"

#: ../main.c:106
#, c-format
msgid "   --quit             Quit player\n"
msgstr "   --quit             Вийти з програвача\n"

#: ../main.c:107
#, c-format
msgid "   --version          Print version info and exit\n"
msgstr "   --version          Показати інформацію про версію і вийти\n"

#: ../main.c:108
#, c-format
msgid "   --play             Start playback\n"
msgstr "   --play             Почати програвання\n"

#: ../main.c:109
#, c-format
msgid "   --stop             Stop playback\n"
msgstr "   --stop             Зупинити програвання\n"

#: ../main.c:110
#, c-format
msgid "   --pause            Pause playback\n"
msgstr "   --pause            Призупинити програвання\n"

#: ../main.c:111
#, c-format
msgid "   --toggle-pause     Toggle pause\n"
msgstr "   --toggle-pause     перемикання паузи\n"

#: ../main.c:112
#, c-format
msgid ""
"   --play-pause       Start playback if stopped, toggle pause otherwise\n"
msgstr ""
"   --play-pause       Почати програвання якщо зупинено, включити паузу в "
"протилежному випадку\n"

#: ../main.c:113
#, c-format
msgid "   --next             Next song in playlist\n"
msgstr "   --next             Наступна пісня в списку програвання\n"

#: ../main.c:114
#, c-format
msgid "   --prev             Previous song in playlist\n"
msgstr "   --prev             Попередня пісня в списку програвання\n"

#: ../main.c:115
#, c-format
msgid "   --random           Random song in playlist\n"
msgstr "   --random           Випадкова пісня в списку програвання\n"

#: ../main.c:116
#, c-format
msgid "   --queue            Append file(s) to existing playlist\n"
msgstr "   --queue            Додати файл(и) до існуючого списку програвання\n"

#: ../main.c:117
#, c-format
msgid ""
"   --gui PLUGIN       Tells which GUI plugin to use, default is \"GTK2\"\n"
msgstr ""
"   --gui PLUGIN       Вказує яке доповнення інтерфейсу використовувати, "
"типовим є \"GTK2\"\n"

#: ../main.c:118
#, c-format
msgid "   --nowplaying FMT   Print formatted track name to stdout\n"
msgstr "   --nowplaying FMT   Показати форматовану назву треку до виводу\n"

#: ../main.c:119
#, c-format
msgid ""
"                      FMT %%-syntax: [a]rtist, [t]itle, al[b]um,\n"
"                      [l]ength, track[n]umber, [y]ear, [c]omment,\n"
"                      copy[r]ight, [e]lapsed\n"
msgstr ""
"                      FMT %%-синтаксис: [a]виконавець, [t]назва, [b]альбом,\n"
"                      [l]тривалість, [n]номер треку, [y]рік, [c]коментар,\n"
"                      [r]копірайт, [e]час що пройшов\n"

#: ../main.c:122
#, c-format
msgid ""
"                      example: --nowplaying \"%%a - %%t\" should print "
"\"artist - title\"\n"
msgstr ""
"Наприклад:  --nowplaying \"%%a - %%t\" повинно показати \"виконавець - назва"
"\"\n"

#: ../main.c:123
#, c-format
msgid "                      for more info, see %s\n"
msgstr "для додаткової інформації див. %s\n"

#: ../main.c:124
#, c-format
msgid "                      NOTE: --nowplaying is deprecated.\n"
msgstr "Примітка: опція --nowplaying застаріла.\n"

#: ../main.c:125
#, c-format
msgid ""
"   --nowplaying-tf FMT  Print formatted track name to stdout, using the new "
"title formatting\n"
msgstr "   --nowplaying FMT   Показати форматовану назву треку до виводу\n"

#: ../main.c:126
#, c-format
msgid ""
"                      FMT syntax: http://github.com/DeaDBeeF-Player/deadbeef/"
"wiki/Title-formatting-2.0\n"
msgstr ""
"                      Синтаксис FMT: http://github.com/DeaDBeeF-Player/"
"deadbeef/wiki/Title-formatting-2.0\n"

#: ../main.c:127
#, c-format
msgid ""
"                      example: --nowplaying-tf \"%%artist%% - %%title%%\" "
"should print \"artist - title\"\n"
msgstr ""
"наприклад: --nowplaying \"%%artist%% - %%title%%\" повинно показати "
"\"виконавець - назва\"\n"

#: ../main.c:128
#, c-format
msgid "   --volume [NUM]     Print or set deadbeef volume level.\n"
msgstr ""
"   --volume [ЧИСЛО]     Вивести або встановити рівень гучності deadbeef.\n"

#: ../main.c:129
#, c-format
msgid ""
"                      The NUM parameter can be specified in percents (if no "
"suffix) or dB [-50, 0].\n"
msgstr ""
"                      Параметр ЧИСЛО може бути вказаний у відсотках (без "
"суфікса) або дБ [-50, 0].\n"

#: ../main.c:130
#, c-format
msgid "                      Examples: --volume 80 or --volume -20dB\n"
msgstr "                      Наприклад: --volume 80 або --volume -20dB\n"

