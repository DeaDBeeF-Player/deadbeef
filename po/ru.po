#: ../main.c:103
#, c-format
msgid "Usage: deadbeef [options] [--] [file(s)]\n"
msgstr "Использование: deadbeef [options] [--] [file(s)]\n"

#: ../main.c:104
#, c-format
msgid "Options:\n"
msgstr "Опции:\n"

#: ../main.c:105
#, c-format
msgid "   --help  or  -h     Print help (this message) and exit\n"
msgstr ""
"   --help  или  -h     Вывести на экран справку (это сообщение) и выйти\n"

#: ../main.c:106
#, c-format
msgid "   --quit             Quit player\n"
msgstr "   --quit              Выйти из плеера\n"

#: ../main.c:107
#, c-format
msgid "   --version          Print version info and exit\n"
msgstr ""
"   --version           Вывести на экран информацию о версии программы и "
"выйти\n"

#: ../main.c:108
#, c-format
msgid "   --play             Start playback\n"
msgstr "   --play              Начать воспроизведение\n"

#: ../main.c:109
#, c-format
msgid "   --stop             Stop playback\n"
msgstr "   --stop              Остановить воспроизведение\n"

#: ../main.c:110
#, c-format
msgid "   --pause            Pause playback\n"
msgstr "   --pause             Приостановить воспроизведение\n"

#: ../main.c:111
#, c-format
msgid "   --toggle-pause     Toggle pause\n"
msgstr "   --toggle-pause      Приостановить воспроизведение\n"

#: ../main.c:112
#, c-format
msgid ""
"   --play-pause       Start playback if stopped, toggle pause otherwise\n"
msgstr ""
"   --play-pause        Начать воспроизведение если остановлено, в противном "
"случае - приостановить\n"

#: ../main.c:113
#, c-format
msgid "   --next             Next song in playlist\n"
msgstr "   --next              Следующая дорожка в плейлисте\n"

#: ../main.c:114
#, c-format
msgid "   --prev             Previous song in playlist\n"
msgstr "   --prev              Предыдущая дорожка в плейлисте\n"

#: ../main.c:115
#, c-format
msgid "   --random           Random song in playlist\n"
msgstr "   --random            Воспроизведение случайной дорожки в плейлисте\n"

#: ../main.c:116
#, c-format
msgid "   --queue            Append file(s) to existing playlist\n"
msgstr "   --queue             Добавить файл(ы) в существующий плейлист\n"

#: ../main.c:117
#, c-format
msgid ""
"   --gui PLUGIN       Tells which GUI plugin to use, default is \"GTK2\"\n"
msgstr ""
"   --gui PLUGIN       выбор плагина интерфейса, по-умолчанию \"GTK2\"\n"

#: ../main.c:118
#, c-format
msgid "   --nowplaying FMT   Print formatted track name to stdout\n"
msgstr ""
"   --nowplaying FMT    Вывести на экран форматированное название дорожки\n"

#: ../main.c:119
#, c-format
msgid ""
"                      FMT %%-syntax: [a]rtist, [t]itle, al[b]um,\n"
"                      [l]ength, track[n]umber, [y]ear, [c]omment,\n"
"                      copy[r]ight, [e]lapsed\n"
msgstr ""
"                       FMT %%-синтаксис: %%a-исполнитель, %%t-название, %%b-"
"альбом,\n"
"                       %%l-длина, %%n-номер дорожки, %%y-год, %%c-"
"комментарий,\n"
"                       %%r-авторские права, %%e-время, которое прошло \n"

#: ../main.c:122
#, c-format
msgid ""
"                      example: --nowplaying \"%%a - %%t\" should print "
"\"artist - title\"\n"
msgstr ""
"                      Например: --nowplaying \"%%a - %%t\" выводит "
"\"исполнитель - название\"\n"

#: ../main.c:123
#, c-format
msgid "                      for more info, see %s\n"
msgstr "                      для дополнительной информации, см. %s\n"

#: ../main.c:124
#, c-format
msgid "                      NOTE: --nowplaying is deprecated.\n"
msgstr "                      ПРИМЕЧАНИЕ: опция --nowplaying устарела.\n"

#: ../main.c:125
#, c-format
msgid ""
"   --nowplaying-tf FMT  Print formatted track name to stdout, using the new "
"title formatting\n"
msgstr ""
"   --nowplaying-tf FMT    Вывести на экран форматированное название дорожки, "
"используя новое форматирование заголовка\n"

#: ../main.c:126
#, c-format
msgid ""
"                      FMT syntax: http://github.com/DeaDBeeF-Player/deadbeef/"
"wiki/Title-formatting-2.0\n"
msgstr ""

#: ../main.c:127
#, c-format
msgid ""
"                      example: --nowplaying-tf \"%%artist%% - %%title%%\" "
"should print \"artist - title\"\n"
msgstr ""
"                       Например: --nowplaying-tf \"%%artist%% - %%title%%\" "
"выводит \"исполнитель - название\"\n"

#: ../main.c:128
#, c-format
msgid "   --volume [NUM]     Print or set deadbeef volume level.\n"
msgstr ""
"   --volume [ЧИСЛО]     Печать или установка уровня громкости deadbeef.\n"

#: ../main.c:129
#, c-format
msgid ""
"                      The NUM parameter can be specified in percents (if no "
"suffix) or dB [-50, 0].\n"
msgstr ""
"                      Параметр ЧИСЛО может быть указан в процентах (если нет "
"суффикса) или дБ [-50, 0].\n"

#: ../main.c:130
#, c-format
msgid "                      Examples: --volume 80 or --volume -20dB\n"
msgstr "                      Примеры: --volume 80 или --volume -20dB\n"

