#, c-format
msgid "Usage: deadbeef [options] [--] [file(s)]\n"
msgstr "Utilisation : deadbeef [options] [--] [fichier(s)]\n"

#: ../main.c:128
#, c-format
msgid "Options:\n"
msgstr "Options :\n"

#: ../main.c:129
#, c-format
msgid "   --help  or  -h     Print help (this message) and exit\n"
msgstr "   --help  ou  -h     Affiche l’aide (ce message) et quitte\n"

#: ../main.c:130
#, c-format
msgid "   --quit             Quit player\n"
msgstr "   --quit             Ferme le lecteur\n"

#: ../main.c:131
#, c-format
msgid "   --version          Print version info and exit\n"
msgstr "   --version          Affiche les informations de version et quitte\n"

#: ../main.c:132
#, c-format
msgid "   --play             Start playback\n"
msgstr "   --play             Démarre la lecture\n"

#: ../main.c:133
#, c-format
msgid "   --stop             Stop playback\n"
msgstr "   --stop             Arrête la lecture\n"

#: ../main.c:134
#, c-format
msgid "   --pause            Pause playback\n"
msgstr "   --pause            Suspend la lecture\n"

#: ../main.c:135
#, c-format
msgid "   --toggle-pause     Toggle pause\n"
msgstr "   --toggle-pause     Bascule sur pause\n"

#: ../main.c:136
#, c-format
msgid ""
"   --play-pause       Start playback if stopped, toggle pause otherwise\n"
msgstr "   --play-pause       Démarre la lecture si elle est arrêtée, sinon bascule sur pause\n"

#: ../main.c:137
#, c-format
msgid "   --next             Next song in playlist\n"
msgstr "   --next             Morceau suivant dans la liste de lecture\n"

#: ../main.c:138
#, c-format
msgid "   --prev             Previous song in playlist\n"
msgstr "   --prev             Morceau précédent dans la liste de lecture\n"

#: ../main.c:139
#, c-format
msgid "   --random           Random song in playlist\n"
msgstr "   --random           Morceau aléatoire dans la liste de lecture\n"

#: ../main.c:140
#, c-format
msgid "   --queue            Append file(s) to existing playlist\n"
msgstr "   --queue            Ajoute le ou les fichiers dans la liste existante\n"

#: ../main.c:141
#, c-format
msgid "   --gui PLUGIN       Tells which GUI plugin to use, default is \"GTK2\"\n"
msgstr "...--gui PLUGIN       Indique le greffon d’interface à utiliser, « GTK2 » par défaut\n"

#: ../main.c:142
#, c-format
msgid "   --nowplaying FMT   Print formatted track name to stdout\n"
msgstr "   --nowplaying FMT   Envoie le nom de la piste formatée dans stdout\n"

#: ../main.c:143
#, c-format
msgid ""
"                      FMT %%-syntax: [a]rtist, [t]itle, al[b]um,\n"
"                      [l]ength, track[n]umber, [y]ear, [c]omment,\n"
"                      copy[r]ight, [e]lapsed\n"
msgstr "                      FMT %%-syntaxe : [a]rtiste, [t]itre, al[b]um,\n                      [l]ongueur, [n]uméro de piste, [y]année, [c]ommentaire,\n                      copy[r]ight, [e]écoulé\n"

#: ../main.c:146
#, c-format
msgid ""
"                      example: --nowplaying \"%%a - %%t\" should print "
"\"artist - title\"\n"
msgstr "                      ex : --nowplaying \"%%a - %%t\" affiche « artiste - titre »\n"

#: ../main.c:147
#, c-format
msgid "                      for more info, see %s\n"
msgstr "pour plus d’informations, voir %s\n"

#: ../main.c:148
#, c-format
msgid "                      NOTE: --nowplaying is deprecated.\n"
msgstr "                      NOTE : --nowplaying est obsolète.\n"

#: ../main.c:149
#, c-format
msgid ""
"   --nowplaying-tf FMT  Print formatted track name to stdout, using the new "
"title formatting\n"
msgstr "   --nowplaying-tf FMT  Envoie le nom de la piste formatée dans stdout, utilisant le nouveau formatage de titre\n"

#: ../main.c:150
#, c-format
msgid ""
"                      FMT syntax: http://github.com/DeaDBeeF-"
"Player/deadbeef/wiki/Title-formatting-2.0\n"
msgstr "                      Syntaxe FMT : http://github.com/DeaDBeeF-Player/deadbeef/wiki/Title-formatting-2.0\n"

#: ../main.c:151
#, c-format
msgid ""
"                      example: --nowplaying-tf \"%%artist%% - %%title%%\" "
"should print \"artist - title\"\n"
msgstr "                      ex : --nowplaying-tf \"%%artist%% - %%title%%\" affiche « artiste - titre »\n\n\n"

#: ../main.c:152
#, c-format
msgid "   --volume [NUM]     Print or set deadbeef volume level.\n"
msgstr "   --volume [NUM]     Affiche ou définit le niveau sonore de deadbeef.\n"

#: ../main.c:153
#, c-format
msgid ""
"                      The NUM parameter can be specified in percents "
"(absolute value or increment/decrement)\n"
msgstr "                      Le paramètre NUM peut être spécifié en pourcentages (valeur absolue ou incrémentation/décrémentation)\n"

#: ../main.c:154
#, c-format
msgid "                      or in dB [-50, 0] (if with suffix).\n"
msgstr "                      ou en dB [-50, 0] (si avec suffixe).\n"

#: ../main.c:155
#, c-format
msgid ""
"                      Examples: --volume 80, --volume +10, --volume -5 or "
"--volume -20dB\n"
msgstr "                      Exemples : --volume 80, --volume +10, --volume -5 ou --volume -20 dB\n"

