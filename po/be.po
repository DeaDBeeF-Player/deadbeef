#: ../main.c:122
#, c-format
msgid "Usage: deadbeef [options] [--] [file(s)]\n"
msgstr "Выкарыстанне: deadbeef [options] [--] [file(s)]\n"

#: ../main.c:123
#, c-format
msgid "Options:\n"
msgstr "Параметры:\n"

#: ../main.c:124
#, c-format
msgid "   --help  or  -h     Print help (this message) and exit\n"
msgstr "   --help  ці  -h     Адлюстраваць даведку (гэты тэкст) і выйсці\n"

#: ../main.c:125
#, c-format
msgid "   --quit             Quit player\n"
msgstr "   --quit              Выйсці з плэера\n"

#: ../main.c:126
#, c-format
msgid "   --version          Print version info and exit\n"
msgstr "   --version           Вывесці інфармацыю пра версію праграмы і выйсці\n"

#: ../main.c:127
#, c-format
msgid "   --play             Start playback\n"
msgstr "   --play              Прайграць\n"

#: ../main.c:128
#, c-format
msgid "   --stop             Stop playback\n"
msgstr "   --stop              Спыніць прайграванне\n"

#: ../main.c:129
#, c-format
msgid "   --pause            Pause playback\n"
msgstr "   --pause             Прыпыніць прайграванне\n"

#: ../main.c:130
#, c-format
msgid "   --toggle-pause     Toggle pause\n"
msgstr "   --toggle-pause     Пераключыць паўзу\n"

#: ../main.c:131
#, c-format
msgid ""
"   --play-pause       Start playback if stopped, toggle pause otherwise\n"
msgstr "--play-pause       Пачаць прайграванне, калі спынена, інакш пераключыць паўзу\n"

#: ../main.c:132
#, c-format
msgid "   --next             Next song in playlist\n"
msgstr "   --next              Наступная песня ў плэйлісце\n"

#: ../main.c:133
#, c-format
msgid "   --prev             Previous song in playlist\n"
msgstr "   --prev              Папярэдняя песня ў плэйлісце\n"

#: ../main.c:134
#, c-format
msgid "   --random           Random song in playlist\n"
msgstr "   --random            Выпадковая песня ў плэйлісце\n"

#: ../main.c:135
#, c-format
msgid "   --queue            Append file(s) to existing playlist\n"
msgstr "   --queue             Дадаць файл(ы) у існы плэйліст\n"

#: ../main.c:136
#, c-format
msgid "   --gui PLUGIN       Tells which GUI plugin to use, default is \"GTK2\"\n"
msgstr "   --gui PLUGIN       Абраць убудову інтэрфейсу, прадвызначана \"GTK2\"\n"

#: ../main.c:137
#, c-format
msgid "   --nowplaying FMT   Print formatted track name to stdout\n"
msgstr "   --nowplaying FMT    Адлюстраваць адфарматыраваную назву трэка\n"

#: ../main.c:138
#, c-format
msgid ""
"                      FMT %%-syntax: [a]rtist, [t]itle, al[b]um,\n"
"                      [l]ength, track[n]umber, [y]ear, [c]omment,\n"
"                      copy[r]ight, [e]lapsed\n"
msgstr "FMT %%-сінтаксіс: %%a-выканаўца, %%t-назва, %%b-альбом,\n                     %%l-даўжыня, %%n-нумар трэка, %%y-год, %%c-каментар,\n                     %%r-аўтарскія правы, %%e-пройдзены час\n"

#: ../main.c:141
#, c-format
msgid ""
"                      example: --nowplaying \"%%a - %%t\" should print "
"\"artist - title\"\n"
msgstr "                      Напрыклад: --nowplaying \"%%a - %%t\" выводзіць \"выканаўца - назва\"\n"

#: ../main.c:142
#, c-format
msgid "                      for more info, see %s\n"
msgstr "                      для дадатковай інфармацыі глядзіце %s\n"

#: ../main.c:143
#, c-format
msgid "                      NOTE: --nowplaying is deprecated.\n"
msgstr "                      НАТАТКА: парметр --nowplaying састарэў.\n"

#: ../main.c:144
#, c-format
msgid ""
"   --nowplaying-tf FMT  Print formatted track name to stdout, using the new "
"title formatting\n"
msgstr "   --nowplaying-tf FMT    Вывесці на экран адфарматаваную назву трэка, выкарыстоўваючы новае фарматаванне загалоўка\n"

#: ../main.c:145
#, c-format
msgid ""
"                      FMT syntax: http://github.com/DeaDBeeF-"
"Player/deadbeef/wiki/Title-formatting-2.0\n"
msgstr "                      Сінтаксіс FMT: http://github.com/DeaDBeeF-Player/deadbeef/wiki/Title-formatting-2.0\n"

#: ../main.c:146
#, c-format
msgid ""
"                      example: --nowplaying-tf \"%%artist%% - %%title%%\" "
"should print \"artist - title\"\n"
msgstr "                      напрыклад: --nowplaying-tf \"%%artist%% - %%title%%\" выводзіць \"выканаўца - назва\"\n"

#: ../main.c:147
#, c-format
msgid "   --volume [NUM]     Print or set deadbeef volume level.\n"
msgstr "   --volume [ЛІК]     Вывад альбо прызначэнне ўзроўню гуку deadbeef.\n"

#: ../main.c:148
#, c-format
msgid ""
"                      The NUM parameter can be specified in percents "
"(absolute value or increment/decrement)\n"
msgstr ""

#: ../main.c:149
#, c-format
msgid "                      or in dB [-50, 0] (if with suffix).\n"
msgstr ""

#: ../main.c:150
#, c-format
