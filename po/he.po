#, c-format
msgid "   --queue            Append file(s) to existing playlist\n"
msgstr "   --queue            Append file(s) to existing playlist\n"

#: ../main.c:141
#, c-format
msgid "   --gui PLUGIN       Tells which GUI plugin to use, default is \"GTK2\"\n"
msgstr "   --gui PLUGIN       Tells which GUI plugin to use, default is \"GTK2\"\n"

#: ../main.c:142
#, c-format
msgid "   --nowplaying FMT   Print formatted track name to stdout\n"
msgstr "   --nowplaying FMT   Print formatted track name to stdout\n"

#: ../main.c:143
#, c-format
msgid ""
"                      FMT %%-syntax: [a]rtist, [t]itle, al[b]um,\n"
"                      [l]ength, track[n]umber, [y]ear, [c]omment,\n"
"                      copy[r]ight, [e]lapsed\n"
msgstr "                      FMT %%-syntax: [a]rtist, [t]itle, al[b]um,\n                      [l]ength, track[n]umber, [y]ear, [c]omment,\n                      copy[r]ight, [e]lapsed\n"

#: ../main.c:146
#, c-format
msgid ""
"                      example: --nowplaying \"%%a - %%t\" should print "
"\"artist - title\"\n"
msgstr "                      example: --nowplaying \"%%a - %%t\" should print \"artist - title\"\n"

#: ../main.c:147
#, c-format
msgid "                      for more info, see %s\n"
msgstr "                      for more info, see %s\n"

#: ../main.c:148
#, c-format
msgid "                      NOTE: --nowplaying is deprecated.\n"
msgstr "                      NOTE: --nowplaying is deprecated.\n"

#: ../main.c:149
#, c-format
msgid ""
"   --nowplaying-tf FMT  Print formatted track name to stdout, using the new "
"title formatting\n"
msgstr "   --nowplaying-tf FMT  Print formatted track name to stdout, using the new title formatting\n"

#: ../main.c:150
#, c-format
