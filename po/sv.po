#: ../main.c:130
#, c-format
msgid "Usage: deadbeef [options] [--] [file(s)]\n"
msgstr "Usage: deadbeef [alternativ] [--] [fil(er)]\n"

#: ../main.c:131
#, c-format
msgid "Options:\n"
msgstr "Flaggor:\n"

#: ../main.c:132
#, c-format
msgid "   --help  or  -h     Print help (this message) and exit\n"
msgstr "   --help  eller  -h     Visa hjälp (detta meddelande) och avsluta\n"

#: ../main.c:133
#, c-format
msgid "   --quit             Quit player\n"
msgstr "   --quit             Stäng spelare\n"

#: ../main.c:134
#, c-format
msgid "   --version          Print version info and exit\n"
msgstr "   --version          Skriv ut versionsinformation och avsluta\n"

#: ../main.c:135
#, c-format
msgid "   --play             Start playback\n"
msgstr "   --play             Påbörja uppspelning\n"

#: ../main.c:136
#, c-format
msgid "   --stop             Stop playback\n"
msgstr "   --stop             Stoppa uppspelning\n"

#: ../main.c:137
#, c-format
msgid "   --pause            Pause playback\n"
msgstr "   --pause            Pausa uppspelning\n"

#: ../main.c:138
#, c-format
msgid "   --toggle-pause     Toggle pause\n"
msgstr "   --toggle-pause     Växla paus\n"

#: ../main.c:139
#, c-format
msgid ""
"   --play-pause       Start playback if stopped, toggle pause otherwise\n"
msgstr "   --play-pause       Starta uppspelningen om den är stoppad, växla paus annars\n"

#: ../main.c:140
#, c-format
msgid "   --next             Next song in playlist\n"
msgstr "   --next             Nästa låt i spellista\n"

#: ../main.c:141
#, c-format
msgid "   --prev             Previous song in playlist\n"
msgstr "   --prev             Föregående låt i spellista\n"

#: ../main.c:142
#, c-format
msgid "   --random           Random song in playlist\n"
msgstr "   --random           Slumpmässig låt i spellista\n"

#: ../main.c:143
#, c-format
msgid "   --queue            Append file(s) to existing playlist\n"
msgstr "   --queue            Lägg till fil(er) i nuvarande spellista\n"

#: ../main.c:144
#, c-format
msgid "   --gui PLUGIN       Tells which GUI plugin to use, default is \"GTK2\"\n"
msgstr "   --gui PLUGIN       Berättar vilken GUI-insticksmodul som ska användas, standard är \"GTK2\"\n"

#: ../main.c:145
#, c-format
msgid "   --nowplaying FMT   Print formatted track name to stdout\n"
msgstr "   --nowplaying FMT   Skriv formaterat spårnamn till standard ut\n"

#: ../main.c:146
#, c-format
msgid ""
"                      FMT %%-syntax: [a]rtist, [t]itle, al[b]um,\n"
"                      [l]ength, track[n]umber, [y]ear, [c]omment,\n"
"                      copy[r]ight, [e]lapsed\n"
msgstr "                      FMT %%-syntax: artist (a), titel (t), album (b),\n                      längd (l), spårnummer (n), år (y), kommentar (c),\n                      utgiven av (r), förfluten tid (e)\n"

#: ../main.c:149
#, c-format
msgid ""
"                      example: --nowplaying \"%%a - %%t\" should print "
"\"artist - title\"\n"
msgstr "                      exempel: --nowplaying \"%%a - %%t\" ska skriva ut \"artist - titel\"\n"

#: ../main.c:150
#, c-format
msgid "                      for more info, see %s\n"
msgstr "                      för mer info, se %s\n"

#: ../main.c:151
#, c-format
msgid "                      NOTE: --nowplaying is deprecated.\n"
msgstr "                      OBS: --nowplaying är föråldrad.\n"

#: ../main.c:152
#, c-format
msgid ""
"   --nowplaying-tf FMT  Print formatted track name to stdout, using the new "
"title formatting\n"
msgstr "   --nowplaying-tf FMT  Skriv ut formaterat spårnamn till stdout med den nya titelformateringen\n"

#: ../main.c:153
#, c-format
msgid ""
"                      FMT syntax: http://github.com/DeaDBeeF-"
"Player/deadbeef/wiki/Title-formatting-2.0\n"
msgstr "                      FMT-syntax: http://github.com/DeaDBeeF-Player/deadbeef/wiki/Title-formatting-2.0\n"

#: ../main.c:154
#, c-format
msgid ""
"                      example: --nowplaying-tf \"%%artist%% - %%title%%\" "
"should print \"artist - title\"\n"
msgstr "                      exempel: --nowplaying-tf \"%%artist%% - %%title%%\" bör skriva ut \"artist - title\"\n"

#: ../main.c:155
#, c-format
msgid "   --volume [NUM]     Print or set deadbeef volume level.\n"
msgstr "   --volume [NUM]     Skriv ut eller ställ in volymnivå för deadbeef.\n"

#: ../main.c:156
#, c-format
msgid ""
"                      The NUM parameter can be specified in percents "
"(absolute value or increment/decrement)\n"
msgstr "                      NUM-parametern kan anges i procent (absolut värde eller ökning/minskning)\n"

#: ../main.c:157
#, c-format
msgid "                      or in dB [-50, 0] (if with suffix).\n"
msgstr "                      eller i dB [-50, 0] (om med suffix).\n"

#: ../main.c:158
#, c-format
msgid ""
"                      Examples: --volume 80, --volume +10, --volume -5 or "
"--volume -20dB\n"
msgstr "                      Exempel: --volume 80, --volume +10, --volume -5 or --volume -20dB\n"

#: ../main.c:159
#, c-format
msgid ""
"   --plugin=[PLUG]    Send commands to a specific plugin. Use PLUG=main to "
"send commands to deadbeef itself.\n"
msgstr "   --plugin=[PLUG]    Skicka kommandon till en specifik insticksmodul. Använd PLUG=main för att skicka kommandon till deadbeef själv.\n"

#: ../main.c:160
#, c-format
msgid ""
"                      To get plugin specific commands use --plugin=[PLUG] "
"--help\n"
msgstr "                      För att få insticksmodul specifika kommandon använd --plugin=[PLUG] --help\n"

#: ../main.c:161
#, c-format
msgid ""
"   --plugin-list      List all available plugins including indication for "
"plugins that support commands.\n"
msgstr "   --plugin-list      Lista alla tillgängliga insticksmoduler inklusive indikationer för insticksmoduler som stöder kommandon.\n"

