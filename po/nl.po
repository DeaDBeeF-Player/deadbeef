#, c-format
msgid "   --version          Print version info and exit\n"
msgstr "   --version       Versie-informatie printen en afsluiten\n"

#: ../main.c:125
#, c-format
msgid "   --play             Start playback\n"
msgstr "   --play         Afspelen starten\n"

#: ../main.c:126
#, c-format
msgid "   --stop             Stop playback\n"
msgstr "   --stop         Afspelen stoppen\n"

#: ../main.c:127
#, c-format
msgid "   --pause            Pause playback\n"
msgstr "   --pause         Afspelen pauzeren\n"

#: ../main.c:128
#, c-format
msgid "   --toggle-pause     Toggle pause\n"
msgstr "   --toggle-pause     Pauzeren/Hervatten\n"

#: ../main.c:129
#, c-format
msgid ""
"   --play-pause       Start playback if stopped, toggle pause otherwise\n"
msgstr "   --play-pause       Start het afspelen indien gestopt of pauzeer\n"

#: ../main.c:130
#, c-format
msgid "   --next             Next song in playlist\n"
msgstr "   --next         Volgend nummer op afspeellijst\n"

#: ../main.c:131
#, c-format
msgid "   --prev             Previous song in playlist\n"
msgstr "   --prev         Vorig nummer op afspeellijst\n"

