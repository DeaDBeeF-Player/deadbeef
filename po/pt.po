#, c-format
msgid ""
"   --gui PLUGIN       Tells which GUI plugin to use, default is \"GTK2\"\n"
msgstr ""
"   --gui PLUGIN       Indica a interface a utilizar, por definição \"GTK2\"\n"
"\n"

#: ../main.c:118
#, c-format
msgid "   --nowplaying FMT   Print formatted track name to stdout\n"
msgstr "   --nowplaying FMT   Grava o nome da faixa para stdout\n"

#: ../main.c:119
#, c-format
msgid ""
"                      FMT %%-syntax: [a]rtist, [t]itle, al[b]um,\n"
"                      [l]ength, track[n]umber, [y]ear, [c]omment,\n"
"                      copy[r]ight, [e]lapsed\n"
msgstr ""
"                      FMT %%-sintaxe: [a]rtista, [t]ítulo, ál[b]um,\n"
"                      [l]duração, [n]úmerodafaixa, [y]ano, [c]omentário,\n"
"                      di[r]eitosdeautor, d[e]corrido\n"

#: ../main.c:122
#, c-format
msgid ""
"                      example: --nowplaying \"%%a - %%t\" should print "
"\"artist - title\"\n"
msgstr ""
"                      exemplo: --nowplaying \"%%a - %%t\" deve imprimir "
"\"artista - título\"\n"

#: ../main.c:123
#, c-format
msgid "                      for more info, see %s\n"
msgstr "para mais informações, consulte %s\n"

#: ../main.c:124
#, c-format
