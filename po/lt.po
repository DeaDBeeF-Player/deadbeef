#: ../main.c:103
#, c-format
msgid "Usage: deadbeef [options] [--] [file(s)]\n"
msgstr "Naudojimas: deadbeef [parametrai] [--] [file(s)]\n"

#: ../main.c:104
#, c-format
msgid "Options:\n"
msgstr "Parametrai:\n"

#: ../main.c:105
#, c-format
msgid "   --help  or  -h     Print help (this message) and exit\n"
msgstr "   --help  ar  -h     Parodo pagalbos langą (šį) ir išeina\n"

#: ../main.c:106
#, c-format
msgid "   --quit             Quit player\n"
msgstr "   --quit             Užverti grotuvą\n"

#: ../main.c:107
#, c-format
msgid "   --version          Print version info and exit\n"
msgstr "   --version     Parodyti versijos informaciją ir išeiti\n"

#: ../main.c:108
#, c-format
msgid "   --play             Start playback\n"
msgstr "   --play             Pradėti atkūrimą\n"

#: ../main.c:109
#, c-format
msgid "   --stop             Stop playback\n"
msgstr "   --stop             Stabdyti atkūrimą\n"

#: ../main.c:110
#, c-format
msgid "   --pause            Pause playback\n"
msgstr "   --pause            Pristabdyti atkūrimą\n"

#: ../main.c:111
#, c-format
msgid "   --toggle-pause     Toggle pause\n"
msgstr "   --toggle-pause     Perjungti pristabdymą\n"

#: ../main.c:112
#, c-format
msgid ""
"   --play-pause       Start playback if stopped, toggle pause otherwise\n"
msgstr "   --play-pause       Jei sustabdyta atkurti, jei grojama sustabdyti\n"

#: ../main.c:113
#, c-format
msgid "   --next             Next song in playlist\n"
msgstr "   --next             Kita grojaraščio daina\n"

#: ../main.c:114
#, c-format
msgid "   --prev             Previous song in playlist\n"
msgstr "   --prev             Ankstesnė grojaraščio daina\n"

#: ../main.c:115
#, c-format
msgid "   --random           Random song in playlist\n"
msgstr "   --random           Atsitiktinė daina grojaraštyje\n"

#: ../main.c:116
#, c-format
msgid "   --queue            Append file(s) to existing playlist\n"
msgstr "   --queue            Pridėti failą(-us) į esamą grojaraštį\n"

#: ../main.c:117
#, c-format
msgid ""
"   --gui PLUGIN       Tells which GUI plugin to use, default is \"GTK2\"\n"
msgstr ""
"   --gui ĮSKIEPIS       Nurodo kurį grafinės naudotojo sąsajos įskiepį "
"naudoti, numatytasis yra „GTK2“\n"

#: ../main.c:118
#, c-format
msgid "   --nowplaying FMT   Print formatted track name to stdout\n"
msgstr "   --nowplaying FMT   Nusiųsti formatuotą takelių sąrašą į stdout\n"

#: ../main.c:119
#, c-format
msgid ""
"                      FMT %%-syntax: [a]rtist, [t]itle, al[b]um,\n"
"                      [l]ength, track[n]umber, [y]ear, [c]omment,\n"
"                      copy[r]ight, [e]lapsed\n"
msgstr ""
"                      FMT %%-sintaksė: [a]rtist, [t]itle, al[b]um,\n"
"                      [l]ength, track[n]umber, [y]ear, [c]omment,\n"
"                      copy[r]ight, [e]lapsed\n"

#: ../main.c:122
#, c-format
msgid ""
"                      example: --nowplaying \"%%a - %%t\" should print "
"\"artist - title\"\n"
msgstr ""
"                      pavyzdys: --nowplaying \"%%a - %%t\" turėtų "
"atvaizduoti \"atlikėjas - pavadinimas\"\n"

#: ../main.c:123
#, c-format
msgid "                      for more info, see %s\n"
msgstr "                      išsamesnei informacija, žiūrėkite %s\n"

#: ../main.c:124
#, c-format
msgid "                      NOTE: --nowplaying is deprecated.\n"
msgstr "                      PASTABA: --nowplaying yra pasenęs.\n"

#: ../main.c:125
#, c-format
msgid ""
"   --nowplaying-tf FMT  Print formatted track name to stdout, using the new "
"title formatting\n"
msgstr ""
"   --nowplaying-tf FMT  Išvesti formatuotą takelio pavadinimą į stdout, "
"naudojant naująjį pavadinimo formatavimą\n"

#: ../main.c:126
#, c-format
msgid ""
"                      FMT syntax: http://github.com/DeaDBeeF-Player/deadbeef/"
"wiki/Title-formatting-2.0\n"
msgstr ""
"                      FMT sintaksė: http://github.com/DeaDBeeF-Player/"
"deadbeef/wiki/Title-formatting-2.0\n"

#: ../main.c:127
#, c-format
msgid ""
"                      example: --nowplaying-tf \"%%artist%% - %%title%%\" "
"should print \"artist - title\"\n"
msgstr ""
"                      pavyzdys: --nowplaying-tf \"%%artist%% - %%title%%\" "
"turėtų atvaizduoti \"atlikėjas - pavadinimas\"\n"

#: ../main.c:128
#, c-format
msgid "   --volume [NUM]     Print or set deadbeef volume level.\n"
msgstr ""
"   --volume [SKAIČIUS]     Parodyti ar nustatyti deadbeef garsio lygį.\n"

#: ../main.c:129
#, c-format
msgid ""
"                      The NUM parameter can be specified in percents (if no "
"suffix) or dB [-50, 0].\n"
msgstr ""
"                      Parametras SKAIČIUS gali būti nurodytas procentais "
"(jei nėra priesagos) arba dB [-50, 0].\n"

#: ../main.c:130
#, c-format
msgid "                      Examples: --volume 80 or --volume -20dB\n"
msgstr "                      Pavyzdžiai: --volume 80 ar --volume -20dB\n"

