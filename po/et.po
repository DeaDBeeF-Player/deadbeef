#, c-format
msgid ""
"   --gui PLUGIN       Tells which GUI plugin to use, default is \"GTK2\"\n"
msgstr ""
"   --gui PLUGIN       Määrab, millist GUI pluginat kasutada, vaikimisi on "
"\"GTK2\"\n"

#: ../main.c:119
#, c-format
msgid "   --nowplaying FMT   Print formatted track name to stdout\n"
msgstr "   --nowplaying FMT   Prindib pala andmed etteantud formaadis\n"

#: ../main.c:120
#, c-format
msgid ""
"                      FMT %%-syntax: [a]rtist, [t]itle, al[b]um,\n"
"                      [l]ength, track[n]umber, [y]ear, [c]omment,\n"
"                      copy[r]ight, [e]lapsed\n"
msgstr ""
"                      FMT %%-süntaks: [a]rtist, [t]iitel, al[b]um,\n"
"                      [l]ength, track[n]umber, [y]ear, [c]omment,\n"
"                      copy[r]ight, [e]lapsed\n"

#: ../main.c:123
#, c-format
msgid ""
"                      example: --nowplaying \"%%a - %%t\" should print "
"\"artist - title\"\n"
msgstr ""
"                      näide: --nowplaying \"%%a - %%t\" peaks printima "
"\"esitaja - pealkiri\"\n"

#: ../main.c:124
#, c-format
msgid "                      for more info, see %s\n"
msgstr "                      rohkema teabe jaoks vaata %s\n"

#: ../main.c:125
#, c-format
msgid "                      NOTE: --nowplaying is deprecated.\n"
msgstr "                      MÄRKUS: --nowplaying on aegunud.\n"

#: ../main.c:126
#, c-format
msgid ""
"   --nowplaying-tf FMT  Print formatted track name to stdout, using the new "
"title formatting\n"
msgstr ""
"   --nowplaying-tf FMT  Prindib pala vormindatud nime stdout-i, kasutades "
"uut pealkirja vormingut\n"

#: ../main.c:127
#, c-format
msgid ""
"                      FMT syntax: http://github.com/DeaDBeeF-Player/deadbeef/"
"wiki/Title-formatting-2.0\n"
msgstr ""
"                      FMT süntaks: http://github.com/DeaDBeeF-Player/"
"deadbeef/wiki/Title-formatting-2.0\n"

#: ../main.c:128
#, c-format
msgid ""
"                      example: --nowplaying-tf \"%%artist%% - %%title%%\" "
"should print \"artist - title\"\n"
msgstr ""
"                      näide: --nowplaying-tf \"%%artist%% - %%title%%\" "
"peaks printima \"esitaja - pealkiri\"\n"

#: ../main.c:129
#, c-format
msgid "   --volume [NUM]     Print or set deadbeef volume level.\n"
msgstr "   --volume [NUM]     Prindib või määrab helitugevuse.\n"

