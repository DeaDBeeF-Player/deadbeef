#: ../src/main.c:139
#, c-format
msgid "Usage: deadbeef [options] [--] [file(s)]\n"
msgstr "Kasutamine: deadbeef [võtmed] [--] [fail(id)]\n"

#: ../src/main.c:140
#, c-format
msgid "Options:\n"
msgstr "Valikud:\n"

#: ../src/main.c:141
#, c-format
msgid "   --help  or  -h     Print help (this message) and exit\n"
msgstr "   --help  või  -h    Prindib abiinfo (praeguse tekst) ja väljub\n"

#: ../src/main.c:142
#, c-format
msgid "   --quit             Quit player\n"
msgstr "   --quit             Väljub mängijast\n"

#: ../src/main.c:143
#, c-format
msgid "   --version          Print version info and exit\n"
msgstr "   --version          Väljastab versiooniteabe ja väljub\n"

#: ../src/main.c:144
#, c-format
msgid "   --play             Start playback\n"
msgstr "   --play             Käivitab esituse\n"

#: ../src/main.c:145
#, c-format
msgid "   --stop             Stop playback\n"
msgstr "   --stop             Seiskab esituse\n"

#: ../src/main.c:146
#, c-format
msgid "   --pause            Pause playback\n"
msgstr "   --pause            Peatab esituse\n"

#: ../src/main.c:147
#, c-format
msgid "   --toggle-pause     Toggle pause\n"
msgstr "   --toggle-pause     Pausi lülitamine\n"

#: ../src/main.c:148
#, c-format
msgid ""
"   --play-pause       Start playback if stopped, toggle pause otherwise\n"
msgstr "   --play-pause       Kui peatatud siis alustab esituse, muul juhul lülitab ümber\n"

#: ../src/main.c:149
#, c-format
msgid "   --next             Next song in playlist\n"
msgstr "   --next             Järgmine esitusloendi lugu\n"

#: ../src/main.c:150
#, c-format
msgid "   --prev             Previous song in playlist\n"
msgstr "   --prev             Eelmine lugu esitusloendist\n"

#: ../src/main.c:151
#, c-format
msgid "   --random           Random song in playlist\n"
msgstr "   --random           Juhuslik lugu esitusloendist\n"

#: ../src/main.c:152
#, c-format
msgid "   --queue            Append file(s) to existing playlist\n"
msgstr "   --queue            Lisab esitusloendisse faili(e)\n"

#: ../src/main.c:153
#, c-format
msgid "   --gui PLUGIN       Tells which GUI plugin to use, default is \"GTK3\"\n"
msgstr ""

#: ../src/main.c:154
#, c-format
msgid "   --nowplaying FMT   Print formatted track name to stdout\n"
msgstr "   --nowplaying FMT   Prindib pala andmed etteantud formaadis\n"

#: ../src/main.c:157
#, c-format
msgid ""
"                      FMT %%-syntax: [a]rtist, [t]itle, al[b]um,\n"
"                      [l]ength, track[n]umber, [y]ear, [c]omment,\n"
"                      copy[r]ight, [e]lapsed\n"
msgstr "                      FMT %%-süntaks: [a]rtist, [t]iitel, al[b]um,\n                      [l]ength, track[n]umber, [y]ear, [c]omment,\n                      copy[r]ight, [e]lapsed\n"

#: ../src/main.c:160
#, c-format
msgid ""
"                      example: --nowplaying \"%%a - %%t\" should print "
"\"artist - title\"\n"
msgstr "                      näide: --nowplaying \"%%a - %%t\" peaks printima \"esitaja - pealkiri\"\n"

#: ../src/main.c:163
#, c-format
msgid "                      for more info, see %s\n"
msgstr "                      rohkema teabe jaoks vaata %s\n"

#: ../src/main.c:165
#, c-format
msgid "                      NOTE: --nowplaying is deprecated.\n"
msgstr "                      MÄRKUS: --nowplaying on aegunud.\n"

#: ../src/main.c:168
#, c-format
msgid ""
"   --nowplaying-tf FMT  Print formatted track name to stdout, using the new "
"title formatting\n"
msgstr "   --nowplaying-tf FMT  Prindib pala vormindatud nime stdout-i, kasutades uut pealkirja vormingut\n"

#: ../src/main.c:171
#, c-format
msgid ""
"                      FMT syntax: http://github.com/DeaDBeeF-"
"Player/deadbeef/wiki/Title-formatting-2.0\n"
msgstr "                      FMT süntaks: http://github.com/DeaDBeeF-Player/deadbeef/wiki/Title-formatting-2.0\n"

#: ../src/main.c:174
#, c-format
msgid ""
"                      example: --nowplaying-tf \"%%artist%% - %%title%%\" "
"should print \"artist - title\"\n"
msgstr "                      näide: --nowplaying-tf \"%%artist%% - %%title%%\" peaks printima \"esitaja - pealkiri\"\n"

#: ../src/main.c:175
#, c-format
msgid "   --volume [NUM]     Print or set deadbeef volume level.\n"
msgstr "   --volume [NUM]     Prindib või määrab helitaseme.\n"

#: ../src/main.c:178
#, c-format
msgid ""
"                      The NUM parameter can be specified in percents "
"(absolute value or increment/decrement)\n"
msgstr "                      NUM parameetrit saab täpsustada protsentides (absoluutväärtus või juurdekasv/vähenemine)\n"

#: ../src/main.c:179
#, c-format
msgid "                      or in dB [-50, 0] (if with suffix).\n"
msgstr "                      või dB-des [-50, 0] (kui järelliitega).\n"

#: ../src/main.c:180
#, c-format
msgid ""
"                      Examples: --volume 80, --volume +10, --volume -5 or "
"--volume -20dB\n"
msgstr "                      Näited: --volume 80, --volume +10, --volume -5 või --volume -20dB\n"

#: ../src/main.c:183
#, c-format
msgid ""
"   --plugin=[PLUG]    Send commands to a specific plugin. Use PLUG=main to "
"send commands to deadbeef itself.\n"
msgstr ""

#: ../src/main.c:184
#, c-format
msgid ""
"                      To get plugin specific commands use --plugin=[PLUG] "
"--help\n"
msgstr ""

#: ../src/main.c:187
#, c-format
msgid ""
"   --plugin-list      List all available plugins including indication for "
"plugins that support commands.\n"
msgstr ""

