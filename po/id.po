#: ../src/main.c:136
#, c-format
msgid "Usage: deadbeef [options] [--] [file(s)]\n"
msgstr "Cara pakai: deadbeef [opsi] [--] [berkas]\n"

#: ../src/main.c:137
#, c-format
msgid "Options:\n"
msgstr "Opsi:\n"

#: ../src/main.c:138
#, c-format
msgid "   --help  or  -h     Print help (this message) and exit\n"
msgstr "   --help  or  -h     Cetak bantuan (pesan ini) dan keluar\n"

#: ../src/main.c:139
#, c-format
msgid "   --quit             Quit player\n"
msgstr "   --quit             Keluar dari pemutar\n"

#: ../src/main.c:140
#, c-format
msgid "   --version          Print version info and exit\n"
msgstr "   --version          Cetak info versi dan keluar\n"

#: ../src/main.c:141
#, c-format
msgid "   --play             Start playback\n"
msgstr "   --play             Mulai memutar\n"

#: ../src/main.c:142
#, c-format
msgid "   --stop             Stop playback\n"
msgstr "   --stop             Berhenti memutar\n"

#: ../src/main.c:143
#, c-format
msgid "   --pause            Pause playback\n"
msgstr "   --pause            Jeda memutar\n"

#: ../src/main.c:144
#, c-format
msgid "   --toggle-pause     Toggle pause\n"
msgstr "   --toggle-pause     Jungkitkan jeda\n"

#: ../src/main.c:145
#, c-format
msgid ""
"   --play-pause       Start playback if stopped, toggle pause otherwise\n"
msgstr "   --play-pause       Mulai memutar bila berhenti, jungkitkan jeda bila tidak\n"

#: ../src/main.c:146
#, c-format
msgid "   --next             Next song in playlist\n"
msgstr "   --next             Lagu selanjutnya di daftar putar\n"

#: ../src/main.c:147
#, c-format
msgid "   --prev             Previous song in playlist\n"
msgstr "   --prev             Lagu sebelumnya di daftar putar\n"

#: ../src/main.c:148
#, c-format
msgid "   --random           Random song in playlist\n"
msgstr "   --random           Acak lagu di daftar putar\n"

#: ../src/main.c:149
#, c-format
msgid "   --queue            Append file(s) to existing playlist\n"
msgstr "   --queue            Tambahkan berkas ke daftar putar yang telah ada\n"

#: ../src/main.c:150
#, c-format
msgid "   --gui PLUGIN       Tells which GUI plugin to use, default is \"GTK2\"\n"
msgstr "   --gui PLUGIN       Memberi tahu plugin GUI mana yang akan digunakan, bakunya adalah \"GTK2\"\n"

#: ../src/main.c:151
#, c-format
msgid "   --nowplaying FMT   Print formatted track name to stdout\n"
msgstr "   --nowplaying FMT   Cetak nama trek terformat ke stdout\n"

#: ../src/main.c:152
#, c-format
msgid ""
"                      FMT %%-syntax: [a]rtist, [t]itle, al[b]um,\n"
"                      [l]ength, track[n]umber, [y]ear, [c]omment,\n"
"                      copy[r]ight, [e]lapsed\n"
msgstr "                      FMT %%-sintaks: [a]rtis, [t]judul, al[b]um,\n                      [l]panjang, [n]omor trek, [y]tahun, [c]komentar,\n                      [r]hak cipta, [e]lama putar\n"

#: ../src/main.c:155
#, c-format
msgid ""
"                      example: --nowplaying \"%%a - %%t\" should print "
"\"artist - title\"\n"
msgstr "                      contoh: --nowplaying \"%%a - %%t\" akan mencetak \"artis - judul\"\n"

#: ../src/main.c:156
#, c-format
msgid "                      for more info, see %s\n"
msgstr "                      untuk info selengkapnya, lihat %s\n"

#: ../src/main.c:157
#, c-format
msgid "                      NOTE: --nowplaying is deprecated.\n"
msgstr "                      CATATAN: --nowplaying tidak digunakan lagi.\n"

#: ../src/main.c:158
#, c-format
msgid ""
"   --nowplaying-tf FMT  Print formatted track name to stdout, using the new "
"title formatting\n"
msgstr "   --nowplaying-tf FMT Mencetak nama trek terformat ke stdout, menggunakan format judul baru\n"

#: ../src/main.c:159
#, c-format
msgid ""
"                      FMT syntax: http://github.com/DeaDBeeF-"
"Player/deadbeef/wiki/Title-formatting-2.0\n"
msgstr "                      Sintaks FMT: http://github.com/DeaDBeeF-Player/deadbeef/wiki/Title-formatting-2.0\n"

#: ../src/main.c:160
#, c-format
msgid ""
"                      example: --nowplaying-tf \"%%artist%% - %%title%%\" "
"should print \"artist - title\"\n"
msgstr "                      contoh: --nowplaying-tf \"%%artist%% - %%title%%\" akan mencetak \"artis - judul\"\n"

#: ../src/main.c:161
#, c-format
msgid "   --volume [NUM]     Print or set deadbeef volume level.\n"
msgstr "   --volume [NUM]      Mencetak atau mengatur level volume deadbeef.\n"

#: ../src/main.c:162
#, c-format
msgid ""
"                      The NUM parameter can be specified in percents "
"(absolute value or increment/decrement)\n"
msgstr "                      Parameter NUM dapat ditentukan dalam persen (nilai absolut atau kenaikan/penurunan)\n"

#: ../src/main.c:163
#, c-format
msgid "                      or in dB [-50, 0] (if with suffix).\n"
msgstr "                      atau dalam dB [-50, 0] (jika dengan akhiran).\n"

#: ../src/main.c:164
#, c-format
msgid ""
"                      Examples: --volume 80, --volume +10, --volume -5 or "
"--volume -20dB\n"
msgstr "                      Contoh: --volume 80, --volume +10, --volume -5, atau --volume -20dB\n"

#: ../src/main.c:165
#, c-format
msgid ""
"   --plugin=[PLUG]    Send commands to a specific plugin. Use PLUG=main to "
"send commands to deadbeef itself.\n"
msgstr "   --plugin=[PLUG]     Mengirim perintah ke plugin tertentu. Gunakan PLUG=main untuk mengirim perintah ke deadbeef itu sendiri.\n"

#: ../src/main.c:166
#, c-format
msgid ""
"                      To get plugin specific commands use --plugin=[PLUG] "
"--help\n"
msgstr "                      Untuk mendapatkan perintah spesifik plugin, gunakan --plugin=[PLUG] --help\n"

#: ../src/main.c:167
#, c-format
msgid ""
"   --plugin-list      List all available plugins including indication for "
"plugins that support commands.\n"
msgstr "   --plugin-list       Daftar semua plugin yang tersedia termasuk indikasi untuk plugin yang mendukung perintah.\n"

