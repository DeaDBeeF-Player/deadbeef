#: ../main.c:104
#, c-format
msgid "Options:\n"
msgstr "Opcións:\n"

#: ../main.c:105
#, c-format
msgid "   --help  or  -h     Print help (this message) and exit\n"
msgstr "   --help  ou  -h     Amosar a axuda (esta mensaxe) e saír\n"

#: ../main.c:106
#, c-format
msgid "   --quit             Quit player\n"
msgstr "   --quit             Pechar o reprodutor\n"

#: ../main.c:107
#, c-format
msgid "   --version          Print version info and exit\n"
msgstr "   --version          Amosar a versión e saír\n"

#: ../main.c:108
#, c-format
msgid "   --play             Start playback\n"
msgstr "   --play             Iniciar a reprodución\n"

#: ../main.c:109
#, c-format
msgid "   --stop             Stop playback\n"
msgstr "   --stop             Deter a reprodución\n"

#: ../main.c:110
#, c-format
msgid "   --pause            Pause playback\n"
msgstr "   --pause            Pausar a reprodución\n"

#: ../main.c:111
#, c-format
msgid "   --toggle-pause     Toggle pause\n"
msgstr "   --toggle-pause     Inverter a pausa\n"

#: ../main.c:112
#, c-format
msgid ""
"   --play-pause       Start playback if stopped, toggle pause otherwise\n"
msgstr ""
"   --play-pause       Iniciar a reprodución se se detén, ou pola contra "
"inverter a pausa\n"

#: ../main.c:113
#, c-format
msgid "   --next             Next song in playlist\n"
msgstr "   --next             Seguinte canción da lista de reprodución\n"

#: ../main.c:114
#, c-format
msgid "   --prev             Previous song in playlist\n"
msgstr "   --prev             Canción anterior da lista de reprodución\n"

#: ../main.c:115
#, c-format
msgid "   --random           Random song in playlist\n"
msgstr "   --random           Canción ao chou da lista de reprodución\n"

#: ../main.c:116
#, c-format
msgid "   --queue            Append file(s) to existing playlist\n"
msgstr ""
"   --queue            Engadir ficheiro(s) á lista de reprodución existente\n"

#: ../main.c:117
#, c-format
msgid ""
"   --gui PLUGIN       Tells which GUI plugin to use, default is \"GTK2\"\n"
msgstr ""
"   --gui PLUGIN  indica que engadido  de interface de usuario usar, o "
"predeterminado é «GTK2»\n"
"\n"

#: ../main.c:118
#, c-format
msgid "   --nowplaying FMT   Print formatted track name to stdout\n"
msgstr "   --nowplaying FMT   Amosar o nome da pista formatado por stdout\n"

#: ../main.c:119
#, c-format
msgid ""
"                      FMT %%-syntax: [a]rtist, [t]itle, al[b]um,\n"
"                      [l]ength, track[n]umber, [y]ear, [c]omment,\n"
"                      copy[r]ight, [e]lapsed\n"
msgstr ""
"                      FMT %%-sintaxe: a: Artista, t: Título, b: Álbum,\n"
"                      l: Duración, n: Pista, y: Ano, c: Comentario,\n"
"                      r: Copyright, e: Tempo restante\n"

#: ../main.c:122
#, c-format
msgid ""
"                      example: --nowplaying \"%%a - %%t\" should print "
"\"artist - title\"\n"
msgstr ""
"                      exemplo: --nowplaying \"%%a - %%t\" imprimiría "
"\"artista - título\"\n"

#: ../main.c:123
#, c-format
msgid "                      for more info, see %s\n"
msgstr "para obter máis información, vexa %s\n"

#: ../main.c:124
#, c-format
msgid "                      NOTE: --nowplaying is deprecated.\n"
msgstr "                      NOTA: --nowplaying está obsoleto.\n"

#: ../main.c:125
#, c-format
msgid ""
"   --nowplaying-tf FMT  Print formatted track name to stdout, using the new "
"title formatting\n"
msgstr ""
"   --nowplaying-tf FMT imprime o nome formatado da pista no stdout, usando o "
"farmatado do novo título\n"

#: ../main.c:126
#, c-format
msgid ""
"                      FMT syntax: http://github.com/DeaDBeeF-Player/deadbeef/"
"wiki/Title-formatting-2.0\n"
msgstr ""

#: ../main.c:127
#, c-format
msgid ""
"                      example: --nowplaying-tf \"%%artist%% - %%title%%\" "
"should print \"artist - title\"\n"
msgstr ""
"                      exemplo: --nowplaying-tf \"%%artist%% - %%title%%\" "
"imprimiría \"artista - título\"\n"

#: ../main.c:128
#, c-format
msgid "   --volume [NUM]     Print or set deadbeef volume level.\n"
