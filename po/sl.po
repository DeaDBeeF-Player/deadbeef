msgid "Usage: deadbeef [options] [--] [file(s)]\n"
msgstr ""

#: ../main.c:104
#, c-format
msgid "Options:\n"
msgstr "Možnosti:\n"

#: ../main.c:105
#, c-format
msgid "   --help  or  -h     Print help (this message) and exit\n"
msgstr "   --help  ali  -h     Izpiše pomoč (to sporočilo).\n"

#: ../main.c:106
#, c-format
msgid "   --quit             Quit player\n"
msgstr "   --quit             Konča predvajalnik.\n"

#: ../main.c:107
#, c-format
msgid "   --version          Print version info and exit\n"
msgstr "   --version          Izpiše podatke o različici.\n"

#: ../main.c:108
#, c-format
msgid "   --play             Start playback\n"
msgstr "   --play             Začne predvajanje.\n"

#: ../main.c:109
#, c-format
msgid "   --stop             Stop playback\n"
msgstr "   --stop             Zaustavi predvajanje.\n"

#: ../main.c:110
#, c-format
msgid "   --pause            Pause playback\n"
msgstr "   --pause            Prekine predvajanje.\n"

#: ../main.c:111
#, c-format
msgid "   --toggle-pause     Toggle pause\n"
msgstr "   --toggle-pause     Začne/prekine predvajanje\n"

#: ../main.c:112
#, c-format
msgid ""
"   --play-pause       Start playback if stopped, toggle pause otherwise\n"
msgstr ""
"   --play-pause       Začne predvajanje, če je zaustavljeno, v nasprotnem "
"primeru ga prekine.\n"

#: ../main.c:113
#, c-format
msgid "   --next             Next song in playlist\n"
msgstr "   --next             Naslednja skladba s seznama predvajanja.\n"

#: ../main.c:114
#, c-format
msgid "   --prev             Previous song in playlist\n"
msgstr "   --prev             Predhodna skladba s seznama predvajanja.\n"

#: ../main.c:115
#, c-format
msgid "   --random           Random song in playlist\n"
msgstr "   --random           Naključna skladba s seznama predvajanja.\n"

#: ../main.c:116
#, c-format
msgid "   --queue            Append file(s) to existing playlist\n"
msgstr "   --queue            Pripne datoteko na seznam predvajanja.\n"

#: ../main.c:117
#, c-format
msgid ""
"   --gui PLUGIN       Tells which GUI plugin to use, default is \"GTK2\"\n"
msgstr ""

#: ../main.c:118
#, c-format
msgid "   --nowplaying FMT   Print formatted track name to stdout\n"
msgstr ""
"   --nowplaying OBLIKA   Izpiše oblikovano ime trenutno predvajane skladbe.\n"

#: ../main.c:119
#, c-format
msgid ""
"                      FMT %%-syntax: [a]rtist, [t]itle, al[b]um,\n"
"                      [l]ength, track[n]umber, [y]ear, [c]omment,\n"
"                      copy[r]ight, [e]lapsed\n"
msgstr ""
"                      OBLIKA (začnite z %%): izv[a]jalec, [t] - naslov, "
"al[b]um,\n"
"                      [l] - trajanje, [n] - številka skladbe, [y] - leto,\n"
"                      [c] - opomba, avto[r]ske pravice, pr[e]tečen čas\n"

#: ../main.c:122
#, c-format
msgid ""
"                      example: --nowplaying \"%%a - %%t\" should print "
"\"artist - title\"\n"
msgstr ""

#: ../main.c:123
#, c-format
msgid "                      for more info, see %s\n"
msgstr ""

#: ../main.c:124
#, c-format
msgid "                      NOTE: --nowplaying is deprecated.\n"
msgstr ""

#: ../main.c:125
#, c-format
msgid ""
"   --nowplaying-tf FMT  Print formatted track name to stdout, using the new "
"title formatting\n"
msgstr ""

#: ../main.c:126
#, c-format
msgid ""
"                      FMT syntax: http://github.com/DeaDBeeF-Player/deadbeef/"
"wiki/Title-formatting-2.0\n"
msgstr ""

#: ../main.c:127
#, c-format
msgid ""
"                      example: --nowplaying-tf \"%%artist%% - %%title%%\" "
"should print \"artist - title\"\n"
msgstr ""

#: ../main.c:128
#, c-format
msgid "   --volume [NUM]     Print or set deadbeef volume level.\n"
msgstr ""

#: ../main.c:129
#, c-format
msgid ""
"                      The NUM parameter can be specified in percents (if no "
"suffix) or dB [-50, 0].\n"
msgstr ""

#: ../main.c:130
#, c-format
msgid "                      Examples: --volume 80 or --volume -20dB\n"
