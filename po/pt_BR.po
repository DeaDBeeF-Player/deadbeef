#: ../src/main.c:136
#, c-format
msgid "Usage: deadbeef [options] [--] [file(s)]\n"
msgstr "Uso: deadbeef [opções] [--] [arquivo(s)]\n"

#: ../src/main.c:137
#, c-format
msgid "Options:\n"
msgstr "Opções:\n"

#: ../src/main.c:138
#, c-format
msgid "   --help  or  -h     Print help (this message) and exit\n"
msgstr "   --help  ou  -h     Imprime a ajuda (esta mensagem) e sai\n"

#: ../src/main.c:139
#, c-format
msgid "   --quit             Quit player\n"
msgstr "   --quit             Fecha o DeaDBeeF\n"

#: ../src/main.c:140
#, c-format
msgid "   --version          Print version info and exit\n"
msgstr "   --version          Imprime informações da versão e sai\n"

#: ../src/main.c:141
#, c-format
msgid "   --play             Start playback\n"
msgstr "   --play             Inicia a reprodução\n"

#: ../src/main.c:142
#, c-format
msgid "   --stop             Stop playback\n"
msgstr "   --stop             Para a reprodução\n"

#: ../src/main.c:143
#, c-format
msgid "   --pause            Pause playback\n"
msgstr "   --pause            Pausa a reprodução\n"

#: ../src/main.c:144
#, c-format
msgid "   --toggle-pause     Toggle pause\n"
msgstr "   --toggle-pause     Pausa/reinicia\n"

#: ../src/main.c:145
#, c-format
msgid ""
"   --play-pause       Start playback if stopped, toggle pause otherwise\n"
msgstr "   --play-pause       Inicia a reprodução se parado, senão pausa/reinicia\n"

#: ../src/main.c:146
#, c-format
msgid "   --next             Next song in playlist\n"
msgstr "   --next             Próxima música na lista de reprodução\n"

#: ../src/main.c:147
#, c-format
msgid "   --prev             Previous song in playlist\n"
msgstr "   --prev             Música anterior na lista de reprodução\n"

#: ../src/main.c:148
#, c-format
msgid "   --random           Random song in playlist\n"
msgstr "   --random           Música aleatória na lista de reprodução\n"

#: ../src/main.c:149
#, c-format
msgid "   --queue            Append file(s) to existing playlist\n"
msgstr "   --queue            Acrescenta arquivos(s) à lista de reprodução existente\n"

#: ../src/main.c:150
#, c-format
msgid "   --gui PLUGIN       Tells which GUI plugin to use, default is \"GTK2\"\n"
msgstr "   --gui PLUGIN       Informa qual plugin de interface para utilizar, o padrão é \"GTK2\"\n"

#: ../src/main.c:151
#, c-format
msgid "   --nowplaying FMT   Print formatted track name to stdout\n"
msgstr "   --nowplaying FMT   Imprime o nome da faixa para a saída padrão\n"

#: ../src/main.c:152
#, c-format
msgid ""
"                      FMT %%-syntax: [a]rtist, [t]itle, al[b]um,\n"
"                      [l]ength, track[n]umber, [y]ear, [c]omment,\n"
"                      copy[r]ight, [e]lapsed\n"
msgstr "                      FMT %%-sintaxe: [a]rtista, [t]ítulo, ál[b]um,\n                      [l]duração, [n]úmero da faixa, [y]ano, [c]omentário,\n                      di[r]eitos autorais, tempo d[e]corrido\n"

#: ../src/main.c:155
#, c-format
msgid ""
"                      example: --nowplaying \"%%a - %%t\" should print "
"\"artist - title\"\n"
msgstr ""

#: ../src/main.c:156
#, c-format
msgid "                      for more info, see %s\n"
msgstr ""

#: ../src/main.c:157
#, c-format
msgid "                      NOTE: --nowplaying is deprecated.\n"
msgstr ""

#: ../src/main.c:158
#, c-format
msgid ""
"   --nowplaying-tf FMT  Print formatted track name to stdout, using the new "
"title formatting\n"
msgstr ""

#: ../src/main.c:159
#, c-format
msgid ""
"                      FMT syntax: http://github.com/DeaDBeeF-"
"Player/deadbeef/wiki/Title-formatting-2.0\n"
msgstr ""

#: ../src/main.c:160
#, c-format
msgid ""
"                      example: --nowplaying-tf \"%%artist%% - %%title%%\" "
"should print \"artist - title\"\n"
msgstr ""

#: ../src/main.c:161
#, c-format
msgid "   --volume [NUM]     Print or set deadbeef volume level.\n"
msgstr ""

#: ../src/main.c:162
#, c-format
msgid ""
"                      The NUM parameter can be specified in percents "
"(absolute value or increment/decrement)\n"
msgstr ""

#: ../src/main.c:163
#, c-format
msgid "                      or in dB [-50, 0] (if with suffix).\n"
msgstr ""

#: ../src/main.c:164
#, c-format
msgid ""
"                      Examples: --volume 80, --volume +10, --volume -5 or "
"--volume -20dB\n"
msgstr ""

#: ../src/main.c:165
#, c-format
msgid ""
"   --plugin=[PLUG]    Send commands to a specific plugin. Use PLUG=main to "
"send commands to deadbeef itself.\n"
msgstr ""

#: ../src/main.c:166
#, c-format
msgid ""
"                      To get plugin specific commands use --plugin=[PLUG] "
"--help\n"
msgstr ""

#: ../src/main.c:167
#, c-format
msgid ""
"   --plugin-list      List all available plugins including indication for "
"plugins that support commands.\n"
msgstr ""

