#: ../main.c:103
#, c-format
msgid "Usage: deadbeef [options] [--] [file(s)]\n"
msgstr "사용법: deadbeef [options] [--] [file(s)]\n"

#: ../main.c:104
#, c-format
msgid "Options:\n"
msgstr "옵션:\n"

#: ../main.c:105
#, c-format
msgid "   --help  or  -h     Print help (this message) and exit\n"
msgstr "   --help  or  -h     도움말(이 메시지) 출력\n"

#: ../main.c:106
#, c-format
msgid "   --quit             Quit player\n"
msgstr "   --quit             프로그램 종료\n"

#: ../main.c:107
#, c-format
msgid "   --version          Print version info and exit\n"
msgstr "   --version          버전정보를 보임\n"

#: ../main.c:108
#, c-format
msgid "   --play             Start playback\n"
msgstr "   --play             재생 시작\n"

#: ../main.c:109
#, c-format
msgid "   --stop             Stop playback\n"
msgstr "   --stop             멈춤\n"

#: ../main.c:110
#, c-format
msgid "   --pause            Pause playback\n"
msgstr "   --pause            일시정지\n"

#: ../main.c:111
#, c-format
msgid "   --toggle-pause     Toggle pause\n"
msgstr "   --toggle-pause     일시정지 토글\n"

#: ../main.c:112
#, c-format
msgid ""
"   --play-pause       Start playback if stopped, toggle pause otherwise\n"
msgstr "   --play-pause       재생이 멈춤상태일 때는 재생 시작, 재생 중일 때는 일시중지\n"

#: ../main.c:113
#, c-format
msgid "   --next             Next song in playlist\n"
msgstr "   --next             재생목록의 다음 곡으로\n"

#: ../main.c:114
#, c-format
msgid "   --prev             Previous song in playlist\n"
msgstr "   --prev             재생목록의 이전 곡으로\n"

#: ../main.c:115
#, c-format
msgid "   --random           Random song in playlist\n"
msgstr "   --random           재생목록 안에서 뒤섞어서 재생\n"

#: ../main.c:116
#, c-format
msgid "   --queue            Append file(s) to existing playlist\n"
msgstr "   --queue            재생목록에 파일 추가\n"

#: ../main.c:117
#, c-format
msgid "   --gui PLUGIN       Tells which GUI plugin to use, default is \"GTK2\"\n"
msgstr "   --gui PLUGIN       어떤 GUI plugin을 사용하고 있는지 알려줌, 기본은 \"GTK2\"\n"

#: ../main.c:118
#, c-format
msgid "   --nowplaying FMT   Print formatted track name to stdout\n"
msgstr "   --nowplaying FMT   포맷된 형식으로 트랙 이름을 표준출력\n"

#: ../main.c:119
#, c-format
msgid ""
"                      FMT %%-syntax: [a]rtist, [t]itle, al[b]um,\n"
"                      [l]ength, track[n]umber, [y]ear, [c]omment,\n"
"                      copy[r]ight, [e]lapsed\n"
msgstr "                      FMT %%-syntax: [a]rtist, [t]itle, al[b]um,\n                      [l]ength, track[n]umber, [y]ear, [c]omment,\n                      copy[r]ight, [e]lapsed\n"

#: ../main.c:122
#, c-format
msgid ""
"                      example: --nowplaying \"%%a - %%t\" should print "
"\"artist - title\"\n"
msgstr "예: --nowplaying \"%%a - %%t\"하면 \"아티스트 - 제목\"으로 나타납니다\n"

#: ../main.c:123
#, c-format
msgid "                      for more info, see %s\n"
msgstr "                      더 많은 정보는 %s 를 보세요\n"

#: ../main.c:124
#, c-format
msgid "                      NOTE: --nowplaying is deprecated.\n"
msgstr "                      NOTE: --nowplaying은 삭제되었습니다.\n"

#: ../main.c:125
#, c-format
msgid ""
"   --nowplaying-tf FMT  Print formatted track name to stdout, using the new "
"title formatting\n"
msgstr "   --nowplaying-tf FMT  새로운 형식으로 트랙 이름을 표준출력 \n"

#: ../main.c:126
#, c-format
msgid ""
"                      FMT syntax: http://github.com/DeaDBeeF-"
"Player/deadbeef/wiki/Title-formatting-2.0\n"
msgstr ""

#: ../main.c:127
#, c-format
msgid ""
"                      example: --nowplaying-tf \"%%artist%% - %%title%%\" "
"should print \"artist - title\"\n"
msgstr "                      예: --nowplaying-tf \"%%artist%% - %%title%%\" 하면 \"아티스트 - 제목\"으로 표시됩니다\n"

#: ../main.c:128
#, c-format
msgid "   --volume [NUM]     Print or set deadbeef volume level.\n"
msgstr ""

#: ../main.c:129
#, c-format
msgid ""
"                      The NUM parameter can be specified in percents (if no "
"suffix) or dB [-50, 0].\n"
msgstr ""

#: ../main.c:130
#, c-format
msgid "                      Examples: --volume 80 or --volume -20dB\n"
msgstr ""

