#, c-format
msgid "   --gui PLUGIN       Tells which GUI plugin to use, default is \"GTK3\"\n"
msgstr ""

#: ../src/main.c:154
#, c-format
msgid "   --nowplaying FMT   Print formatted track name to stdout\n"
msgstr "   --nowplaying FMT   Skriv formatert spornavn til stdout\n"

#: ../src/main.c:157
#, c-format
msgid ""
"                      FMT %%-syntax: [a]rtist, [t]itle, al[b]um,\n"
"                      [l]ength, track[n]umber, [y]ear, [c]omment,\n"
"                      copy[r]ight, [e]lapsed\n"
msgstr "                      FMT %%-syntaks: [a]rtist, [t]itle, al[b]um,\n                      [l]ength, track[n]umber, [y]ear, [c]omment,\n                      copy[r]ight, [e]lapsed\n"

#: ../src/main.c:160
#, c-format
msgid ""
"                      example: --nowplaying \"%%a - %%t\" should print "
"\"artist - title\"\n"
msgstr "                      eksempel: --nowplaying \"%%a - %%t\" vil skrive \"artist - tittel\"\n"

#: ../src/main.c:163
#, c-format
